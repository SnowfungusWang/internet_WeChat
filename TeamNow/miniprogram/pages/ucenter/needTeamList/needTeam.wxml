<!--miniprogram/pages/ucenter/needTeam/needTeam.wxml-->
<text>我的组队申请</text>
  <view class='body'>
    <view class='list-col' wx:for="{{list}}" bindtap='toDetailsTap' data-id="{{item._id}}">
      <i-card full='true' title="{{item.title}}" extra="来自 {{ item.school}}">
        <view slot="content" class="card_content">
          <text> 请求人：{{ item.name }}\n\n</text>
          <text> 联系方式：\n{{ item.contact }}\n\n</text>
          <text> 自我描述：\n{{ item.selfDescription }}\n\n</text>
          <text> 备注：\n{{ item.remark }}\n\n</text>
        </view>
        <view slot="footer">
          <text> 发送时间： {{ item.time }}\n\n</text>
        </view>
      </i-card>
    </view>
  </view>